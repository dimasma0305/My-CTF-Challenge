HOSTNAME="ctf.ukmpcc.org"
adds:
	bash ./.ctf/ctf.sh add
installs:
	ctf challenge install
syncs:
	ctf challenge sync
deploy:
	bash ./.ctf/deploy.sh
stop:
	bash ./.ctf/stop.sh
hidden-all:
	bash ./.ctf/hidden.sh
set-hostname:
	bash ./.ctf/set_host.sh "${HOSTNAME}"
install-dependency:
	bash ./.dependency/blockchain/images/build.sh
